{
  "name": "coder-reactjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "glightbox": "^3.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.8.2",
    "react-scripts": "^5.0.1",
    "simplebar-react": "^3.3.2",
    "swiper": "^11.2.10",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "sass:watch": "sass --watch src/styles/styles.scss:public/styles.css --style=compressed --no-source-map",
    "sass:build": "sass src/styles/styles.scss:public/styles.css --style=compressed --no-source-map",
    "dev": "concurrently -k -n APP,SASS -c green,magenta \"react-scripts start\" \"npm:sass:watch\"",
    "prebuild": "npm run sass:build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "tailwindcss": "^4.1.11"
  }
